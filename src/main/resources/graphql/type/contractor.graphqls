type Contractor implements baseEntity{
    id: ID!
    name: String!
    email: String!
    website: String
    status: ContractorStatus!
    address: Address
    contactNumber: String
    contractorSkills: [ContractorSkill!]!
    company: Company
    expectedPay: Int
    expectedPayCurrency: String
    resume: PdfDocument
    avatar: Image
    projectContractors: [ProjectContractor!]!
    createdOn: Date
    modifiedOn: Date
}